<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:parentTag="dji.v5.ux.core.widget.hsi.AttitudeDisplayWidget">

    <dji.v5.ux.core.ui.hsi.dashboard.AttitudeDashBoard
        android:id="@+id/pfd_attitude_dash_board"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:uxsdk_current_value="20"
        app:uxsdk_dash_board_align="left"
        app:uxsdk_name="ALT"
        app:uxsdk_offset_per_unit="5"
        app:uxsdk_unit="m"
        app:uxsdk_barrier_distance_text_align="vertical"
        app:uxsdk_framework_height="@dimen/uxsdk_104_dp"
        app:uxsdk_avoidance_max_height="@dimen/uxsdk_55_dp"
        app:uxsdk_calibration_framework_padding_start="@dimen/uxsdk_11_dp"
        app:uxsdk_current_calibration_text_size="@dimen/uxsdk_text_size_medium_large"
        app:uxsdk_name_text_size="@dimen/uxsdk_text_size_normal"
        app:uxsdk_unit_text_size="@dimen/uxsdk_text_size_between_small_tiny"
        app:uxsdk_pointer_width="@dimen/uxsdk_97_dp"
        app:uxsdk_pointer_height="@dimen/uxsdk_31_dp"
        app:uxsdk_barrier_indicator_width="@dimen/uxsdk_16_dp"
        app:uxsdk_distance_predict_line_width="@dimen/uxsdk_3_dp"
        app:uxsdk_barrier_indicator_height="@dimen/uxsdk_2_dp"
        app:uxsdk_avoidance_indicator_width="@dimen/uxsdk_6_dp"
        app:uxsdk_barrier_distance_text_size="@dimen/uxsdk_text_size_small"
        app:uxsdk_max_perception_distance_in_meter="30"
        app:uxsdk_pointer_line_inner_width="0dp"
        app:uxsdk_pointer_line_outer_width="@dimen/uxsdk_8_dp"
        app:uxsdk_pointer_triangle_width="@dimen/uxsdk_6_dp"
        app:uxsdk_pointer_divider_bottom_width="@dimen/uxsdk_35_dp"
        app:uxsdk_pointer_divider_top_width="@dimen/uxsdk_44_dp"
        app:uxsdk_show_calibration_framework="false"
        app:uxsdk_max_flight_height_indicator_height="@dimen/uxsdk_3_dp"
        app:uxsdk_max_flight_height_indicator_width="@dimen/uxsdk_32_dp"
        app:uxsdk_return_to_home_indicator_width="@dimen/uxsdk_32_dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/pfd_vs_text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/uxsdk_5_dp"
        android:text="VS"
        android:textColor="@color/uxsdk_pfd_main_color"
        app:layout_constraintVertical_bias="0.85"
        app:layout_constraintVertical_chainStyle="packed"
        android:textSize="@dimen/uxsdk_text_size_normal_medium"
        android:shadowColor="@color/uxsdk_black_47_percent"
        android:shadowDx="2"
        android:shadowDy="2"
        android:shadowRadius="2"
        app:layout_constraintEnd_toEndOf="@+id/pfd_asl_text"
        app:layout_constraintStart_toStartOf="@+id/pfd_asl_text"
        app:layout_constraintBottom_toTopOf="@+id/pfd_asl_text"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/pfd_vs_value"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/uxsdk_3_dp"
        android:text="@string/uxsdk_not_a_num"
        android:textColor="@color/uxsdk_pfd_main_color"
        android:textSize="@dimen/uxsdk_text_size_normal_medium"
        android:shadowColor="@color/uxsdk_black_47_percent"
        android:shadowDx="2"
        android:shadowDy="2"
        android:shadowRadius="2"
        app:layout_constraintEnd_toStartOf="@id/pfd_vs_text"
        app:layout_constraintTop_toTopOf="@+id/pfd_vs_text" />

    <TextView
        android:id="@+id/pfd_asl_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ASL"
        android:textColor="@color/uxsdk_pfd_main_color"
        android:textSize="@dimen/uxsdk_text_size_normal_medium"
        android:shadowColor="@color/uxsdk_black_47_percent"
        android:shadowDx="2"
        android:shadowDy="2"
        android:shadowRadius="2"
        app:layout_constraintStart_toEndOf="@id/pfd_asl_value"
        app:layout_constraintEnd_toEndOf="@id/pfd_attitude_dash_board"
        app:layout_constraintTop_toBottomOf="@+id/pfd_vs_text"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="@dimen/uxsdk_20_dp" />

    <TextView
        android:id="@+id/pfd_asl_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/uxsdk_3_dp"
        android:text="@string/uxsdk_not_a_num"
        android:textColor="@color/uxsdk_pfd_main_color"
        android:textSize="@dimen/uxsdk_text_size_normal_medium"
        android:shadowColor="@color/uxsdk_black_47_percent"
        android:shadowDx="2"
        android:shadowDy="2"
        android:shadowRadius="2"
        app:layout_constraintHorizontal_bias="1"
        app:layout_constraintEnd_toStartOf="@id/pfd_vs_text"
        app:layout_constraintTop_toTopOf="@id/pfd_asl_text" />


</merge>
